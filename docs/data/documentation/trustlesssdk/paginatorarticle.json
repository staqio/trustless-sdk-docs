":[{"type":"paragraph","inlineContent":[{"type":"reference","identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/fetchNextPage(completion:)","isActive":true},{"type":"text","text":", you can use this method to fetch next pages,"}]}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"to perform retrieval of data in pages."}]},{"style":"note","type":"aside","content":[{"inlineContent":[{"type":"text","text":""},{"code":"fetchNextPage(completion:)","type":"codeVoice"},{"text":" should be called after ","type":"text"},{"code":"fetchFirstPage(completion:)","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"name":"Note"},{"inlineContent":[{"type":"text","text":"To retrive results Paginator provides "},{"type":"reference","identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/results","isActive":true},{"type":"text","text":", this property includes all results."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"To handle result you can assign handlers to paginator:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/setResultsHandler(_:)","type":"reference","isActive":true},{"type":"text","text":", to handle results;"}]}]},{"content":[{"inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/setFailureHandler(_:)"},{"type":"text","text":", to handle errors."}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Also Paginator provides additional properties:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/pageNumber"},{"type":"text","text":", s