()","        tableView.insertRows(at: indexPaths, with: .automatic)","        tableView.endUpdates()","    }","","    \/\/ TableViewDelegate","","    override func tableView(_ tableView: UITableView, numberOfRowsInSection section: Int) -> Int {","        return paginator.results.count","    }","","    override func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {","        return UITableViewCell()","    }","","    override func tableView(_ tableView: UITableView, didSelectRowAt indexPath: IndexPath) {}","","    \/\/ ScrollViewDelegate","","    override func scrollViewDidScroll(_ scrollView: UIScrollView) {","        guard !paginator.reachedLastPage else { return }","","        let offsetY = scrollView.contentOffset.y","        let contentHeight = scrollView.contentSize.height","        let height = scrollView.frame.size.height","","        if offsetY > contentHeight - height * 1, !(paginator.requestStatus == .inProgress) {","            cancelCallback = paginator.fetchNextPage { result in","                self.endLoadingMoreItems()","                switch result {","                case .success(let data):","                    self.insertElements(newItemsCount: data.count)","                case .failure:","                    break","                }","            }","        }","    }","}"],"syntax":"swift","type":"codeListing"}],"kind":"content"}],"references":{"doc://com.staq.TrustlessSDK/documentation/TrustlessSDK/Paginator/setFailureHandler(_:)":{"identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/setFailureHandler(_:)","kind":"symbol","type":"topic","url":"\/documentation\/trustlesssdk\/paginator\/setfailurehandler(_:)","abstract":[{"text":"Sets a handler to be called when a fetch operation fails.","type":"text"}],"fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"setFailureHandler","kind":"identifier"},{"kind":"text","text":"("},{"text":"PaginatorFa