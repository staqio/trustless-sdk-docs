nsertRows(at: indexPaths, with: .automatic)","        tableView.endUpdates()","    }","","    \/\/ TableViewDelegate","","    override func tableView(_ tableView: UITableView, numberOfRowsInSection section: Int) -> Int {","        return paginator.results.count","    }","","    override func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {","        return UITableViewCell()","    }","","    override func tableView(_ tableView: UITableView, didSelectRowAt indexPath: IndexPath) {}","","    \/\/ ScrollViewDelegate","","    override func scrollViewDidScroll(_ scrollView: UIScrollView) {","        guard !paginator.reachedLastPage else { return }","","        let offsetY = scrollView.contentOffset.y","        let contentHeight = scrollView.contentSize.height","        let height = scrollView.frame.size.height","","        if offsetY > contentHeight - height * 1, !(paginator.requestStatus == .inProgress) {","            cancelCallback = paginator.fetchNextPage { result in","                self.endLoadingMoreItems()","                switch result {","                case .success(let data):","                    self.insertElements(newItemsCount: data.count)","                case .failure:","                    break","                }","            }","        }","    }","}"]}],"kind":"content"}],"sections":[],"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://com.staq.TrustlessSDK/documentation/TrustlessSDK/RequestSendingArticle":{"role":"article","type":"topic","title":"Request Sending","identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/RequestSendingArticle","abstract":[{"type":"text","text":"Sending requests to and receiving responses from Staq Server APIs: A detailed walkthrough."}],"url":"\/documentation\/trustlesssdk\/requestsendingarticle","kind":"article"},"doc://com.staq.TrustlessSDK/documentation/TrustlessSDK/RequestStatus":{"type":"topic","navigatorTitle":[{"kind":"identifie