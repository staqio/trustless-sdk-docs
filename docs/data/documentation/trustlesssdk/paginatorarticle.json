hs = (startIndex ..< paginator.results.count).map { IndexPath(row: $0, section: 0) }","","        tableView.beginUpdates()","        tableView.insertRows(at: indexPaths, with: .automatic)","        tableView.endUpdates()","    }","","    \/\/ TableViewDelegate","","    override func tableView(_ tableView: UITableView, numberOfRowsInSection section: Int) -> Int {","        return paginator.results.count","    }","","    override func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {","        return UITableViewCell()","    }","","    override func tableView(_ tableView: UITableView, didSelectRowAt indexPath: IndexPath) {}","","    \/\/ ScrollViewDelegate","","    override func scrollViewDidScroll(_ scrollView: UIScrollView) {","        guard !paginator.reachedLastPage else { return }","","        let offsetY = scrollView.contentOffset.y","        let contentHeight = scrollView.contentSize.height","        let height = scrollView.frame.size.height","","        if offsetY > contentHeight - height * 1, !(paginator.requestStatus == .inProgress) {","            cancelCallback = paginator.fetchNextPage { result in","                self.endLoadingMoreItems()","                switch result {","                case .success(let data):","                    self.insertElements(newItemsCount: data.count)","                case .failure:","                    break","                }","            }","        }","    }","}"]}],"kind":"content"}],"references":{"doc://com.staq.TrustlessSDK/documentation/TrustlessSDK/RequestStatus":{"title":"RequestStatus","type":"topic","kind":"symbol","role":"symbol","abstract":[{"text":"Represents the current status of a paginated data request.","type":"text"}],"url":"\/documentation\/trustlesssdk\/requeststatus","identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/RequestStatus","fragments":[{"text":"enum","kind":"keyword"},{"text":" ","kind":"text"},{"text":"RequestStatus","kind