urn paginator.results.count","    }","","    override func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {","        return UITableViewCell()","    }","","    override func tableView(_ tableView: UITableView, didSelectRowAt indexPath: IndexPath) {}","","    \/\/ ScrollViewDelegate","","    override func scrollViewDidScroll(_ scrollView: UIScrollView) {","        guard !paginator.reachedLastPage else { return }","","        let offsetY = scrollView.contentOffset.y","        let contentHeight = scrollView.contentSize.height","        let height = scrollView.frame.size.height","","        if offsetY > contentHeight - height * 1, !(paginator.requestStatus == .inProgress) {","            cancelCallback = paginator.fetchNextPage { result in","                self.endLoadingMoreItems()","                switch result {","                case .success(let data):","                    self.insertElements(newItemsCount: data.count)","                case .failure:","                    break","                }","            }","        }","    }","}"],"type":"codeListing"}]}],"variants":[{"paths":["\/documentation\/trustlesssdk\/paginatorarticle"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK"]]},"identifier":{"url":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/PaginatorArticle","interfaceLanguage":"swift"},"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://com.staq.TrustlessSDK/documentation/TrustlessSDK/Paginator/totalPages":{"role":"symbol","abstract":[{"text":"The total number of pages.","type":"text"}],"identifier":"doc:\/\/com.staq.TrustlessSDK\/documentation\/TrustlessSDK\/Paginator\/totalPages","title":"totalPages","url":"\/documentation\/trustlesssdk\/paginator\/totalpages","kind":"symbol","type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"totalPag